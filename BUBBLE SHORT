#include<iostream>
#include<stdio.h>
int main ()
{
	int i,j,A[50],temp,n;
	printf("enter the number of element");
	scanf("%d",&n);
	printf("enter the n array of elements");
	for(i=0;i<n;i++)
	{
		scanf("%d",&A[i]);
	}
		for(j=0;j<n-1-i;j++)
			if(A[j]>A[j+1])
		{
				temp=A[j];
				A[j]=A[j+1];
				A[j+1]=temp;
				
			}
		}
	
}
	printf("the sorted array element");
		for(i=0;i<n-1;i++)
		{
		 printf("%d\n",A[i]);}
	}
		 
	
}
